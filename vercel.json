{
    "buildCommand": "cd ui && pnpm run build",
    "outputDirectory": "ui/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/bridge?path=$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/bridge.ts": {
            "maxDuration": 60
        }
    }
}